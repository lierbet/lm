<h1>Editing pagina</h1>

<% form_for(@pagina) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :titulo %><br />
    <%= f.text_field :titulo %>
  </p>
  <p>
    <%= f.label :apelido %><br />
    <%= f.text_field :apelido %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @pagina %> |
<%= link_to 'Back', paginas_path %>